<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Remgeta - <%= product.Label %> <%= product.Description %></title>
    <%- include('Scripts.ejs') %>
</head>
<body>
<!--////////////////////////////////////////////////////////////////-->

<%- include('RemgetaMenu.ejs') %>

<div class="container linija">
    <div class="row">
        <div class="col-md-12">
            <span class="page-title">PARDUOTUVĖ</span>
            <p class="menuTitle"> <a class="menuTitleItem" href="/parduotuve">Parduotuvė</a> / <a class="menuTitleItem" href="../../<%= category.Name %>"><%= category.Label%></a> / <a class="menuTitleItem" href="../<%= subcategory.Name%>"><%= subcategory.Label%></a> / <%= product.Label%></p>
            <div class="text-background">
                <ul class="tekstas">
                    <li><%= product.Label %></li>
                    <li><%= product.Description %></li>
                    <li class="lastText"><%= product.Price %> Eur</li>
                </ul>
            </div>
        </div>
    </div>
    <hr>
</div>
